{
  "permissions": {
    "allow": [
      "MCP(code-RAG:*)"
    ]
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "WebFetch",
        "hooks": [
          {
            "type": "command",
            "command": "python C:/Users/Nix/.claude/hooks/fetch-spa-intercept.py",
            "timeout": 120
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "everything-claude-code@everything-claude-code": true
  }
}
